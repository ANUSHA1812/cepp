#include<conio.h>
#include<stdio.h>
int main()
{
int n,arr[10],c,d,t;
printf("enter the number of ewlements");
scanf("%d",&n);
printf("enter the elements");
for(c=0;c<n;c++)
{
  scanf("%d",&arr[c]);
}
for(c=1;c<=n-1;c++)
{
  d=c;
}
while(d>0&& arr[d]<arr[d-1]
{
  t=arr[d];
  arr[d]=arr[d-1];
  arr[d-1]=t;
  d--;
}

getch();
}
